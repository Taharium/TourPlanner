<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <OutputType>WinExe</OutputType>
    <TargetFramework>net7.0-windows</TargetFramework>
    <Nullable>enable</Nullable>
    <UseWPF>true</UseWPF>
    <ApplicationIcon>Assets\Icon\Tour_Planner_Icon.ico</ApplicationIcon>
  </PropertyGroup>

  <ItemGroup>
    <None Remove="Assets\Images\Tour.png" />
  </ItemGroup>

  <ItemGroup>
    <Resource Include="Assets\Icon\Tour_Planner_Icon.ico" />
  </ItemGroup>

  <ItemGroup>
    <Resource Include="Assets\Images\Tour.png">
      <CopyToOutputDirectory>Never</CopyToOutputDirectory>
    </Resource>
  </ItemGroup>

</Project>
